{
  "name": "adopt-me",
  "version": "1.0.0",
  "description": "An App to practice basic and intermediate React",
  "main": "index.js",
  "scripts": {
    "build": "parcel build --public-url ./dist/ src/index.html",
    "dev": "parcel src/index.html",
    "format": "prettier \"src/**/*.{js,html}\" --write",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "test": "jest --silent",
    "test:update": "jest --silent -u",
    "test:watch": "jest --silent --watch",
    "test:coverage": "jest --silent --coverage",
    "start": "npm run build && babel-node server/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xspired01/adoptMe.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/xspired01/adoptMe/issues"
  },
  "homepage": "https://github.com/xspired01/adoptMe#readme",
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^6.0.3",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.1.2",
    "jest": "^24.9.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.18.2",
    "react-test-renderer": "^16.10.2",
    "react-testing-library": "^8.0.1"
  },
  "dependencies": {
    "@babel/node": "^7.6.3",
    "@frontendmasters/pet": "^1.0.3",
    "@reach/router": "^1.2.1",
    "@testing-library/react": "^9.3.0",
    "babel-cli": "^6.26.0",
    "express": "^4.17.1",
